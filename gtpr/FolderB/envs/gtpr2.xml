<mujoco model="GTPR">
    <!-- set some defaults for units and lighting -->
    <compiler angle="radian" meshdir="."/>
 
    <!-- import our stl files -->
    <asset>
        <mesh file="base_link.STL" />
        <mesh file="A1.STL" />
        <mesh file="A2.STL" />
        <mesh file="A3.STL" />
        <mesh file="L1-L.STL" />
        <mesh file="L1-R.STL" />
        <mesh file="L2-L.STL" />
        <mesh file="L2-R.STL" />
        <mesh file="U1.STL" />
        <mesh file="U1L.STL" />
        <mesh file="U1R.STL" />
        <mesh file="U2.STL" />
        <mesh file="U2L.STL" />
        <mesh file="U2R.STL" />
        <mesh file="U3.STL" />
