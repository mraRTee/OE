						<!-- <body name="WL1-2" pos="0 0 0"> -->
							<!-- <joint name="joint2" axis="1 1 1" pos="0 0 0"/> -->
							<!-- <joint name="joint3" axis="1 0 0" pos="0 0 0"/> -->
							<!-- <geom name="WL1-2" type="mesh" mesh="WL1-2" pos="0 0 0" euler="0  1.57 0"/> -->
							<!-- <inertial pos="0 0 0" mass="0.75" diaginertia="1 1 1"/> -->
						<!-- </body> -->
					
					<!-- </body> -->
				
				</body>
				
				
        
		</body>
		
    </worldbody>
 
    <!-- attach actuators to joints -->
    <actuator>
        <motor name="joint0_motor" joint="joint0"/>
        <motor name="joint1_motor" joint="joint1"/>
        <!-- <motor name="joint2_motor" joint="joint2"/> -->
        <!-- <motor name="joint3_motor" joint="joint3"/> -->
    </actuator>
 
</mujoco>