        <mesh file="WL3-1.STL" />
        <mesh file="WL3-2.STL" />
    </asset>
 
    <!-- GTPR DEF -->
    <worldbody>
        <!-- lights-->
        <light directional="true" pos="-0.5 0.5 3" dir="0 0 -1" />
        <!-- floor -->
        <geom name="floor" pos="0 0 0" size="1 1 1" type="plane" rgba="100 100 100 0.5"/>
        
        <body name="base_link" pos="0 0 0.2" >
			<geom name="link0" type="mesh" mesh="base_link" pos="-2.3525E-17 -0.005 2.3559E-17" rgba="0.7451 0.7451 0.7451 1" euler="1.57 0 0"/>
				
				<body name="A1" pos="0 0 0">
					<joint name="joint0" axis="1 1 1" pos="0 0 0"/>
					<joint name="joint1" axis="1 0 0" pos="0 0 0"/> 
					<geom name="A1" type="mesh" mesh="A1" rgba="0.7451 0.7451 0.7451 1" pos="5.3777E-05 4.4185E-09 0.1" euler="0 0 0"/>
					<inertial pos="0 0 0" mass="0.75" diaginertia="1 1 1"/>
					
